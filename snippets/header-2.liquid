<!-- header fixed -->
<div class="wrap_header fixed-header2 trans-0-4">
  <!-- Logo -->
  {% include 'logo' %}
  <!-- Menu -->
  <div class="wrap_menu">
    <nav class="menu">
      {% include 'main-menu' %}
    </nav>
  </div>
  <!-- Header Icon -->
  <div class="header-icons">
    {% if shop.customer_accounts_enabled %}
        {% if customer %}
          <a href="/account" class="header-wrapicon1 dis-block">
            <img src="{{ 'icon-header-01.png' | asset_url }}" class="header-icon1" alt="ICON">
          </a>
					<div class="header-dropdwn">
						<a href="/account/logout">{{ 'customer.login.logout' | t }}</a>
					</div><!-- header-dropdwn -->
        {% else %}
          <a href="/account/login" class="header-wrapicon1 dis-block">
            <img src="{{ 'icon-header-01.png' | asset_url }}" class="header-icon1" alt="ICON">
          </a>
        {% endif %}
    {% endif %}
    <span class="linedivide1"></span>
    <div class="header-wrapicon2">
      {% include 'header-cart' %}
    </div>
  </div>
</div>
<!-- top noti -->
<div class="flex-c-m size22 bg0 s-text21 pos-relative">
  {% if section.settings.use_top_bar %}
  {{ section.settings.top_bar_message }}
  {% endif %}
  <button class="flex-c-m pos2 size23 colorwhite eff3 trans-0-4 btn-romove-top-noti">
    <i class="fa fa-remove fs-13" aria-hidden="true"></i>
  </button>
</div>
<!-- Header -->
<header class="header2">
  <!-- Header desktop -->
  <div class="container-menu-header-v2 p-t-26">
    <div class="topbar2">
      <div class="topbar-social">
        {% include 'social-button-header' %}
      </div>
      <!-- Logo2 -->
      {% if section.settings.logo %}
      <a href="/" itemprop="url" class="logo2">
        <img src="{{ section.settings.logo | img_url: 'master' }}"
             alt="{{ section.settings.logo.alt | default: shop.name }}"
             itemprop="logo" class=""  />
      </a>
      {% else %}
      <a href="/" itemprop="url" class="navbar-brand img-responsive">{{ shop.name }}</a>
      {% endif %}
      <div class="topbar-child2">
        {% if section.settings.show_email %}
        <span class="topbar-email">
          {{ section.settings.header_email_text }}
        </span>
        {% endif %}
        <div class="topbar-language rs1-select2">
          {% include 'header-currency' %}
        </div>
        <!--  -->
        {% if shop.customer_accounts_enabled %}
            {% if customer %}
              <a href="/account" class="header-wrapicon1 dis-block m-l-30">
                <img src="{{ 'icon-header-01.png' | asset_url }}" class="header-icon1" alt="ICON">
              </a>
							<div class="header-dropdwn">
								<a href="/account/logout">{{ 'customer.login.logout' | t }}</a>
							</div><!-- header-dropdwn -->
            {% else %}
              <a href="/account/login" class="header-wrapicon1 dis-block m-l-30">
                <img src="{{ 'icon-header-01.png' | asset_url }}" class="header-icon1" alt="ICON">
              </a>
            {% endif %}
        {% endif %}
        <span class="linedivide1"></span>
        <div class="header-wrapicon2 m-r-13">
          {% include 'header-cart' %}
        </div>
      </div>
    </div>
    <div class="wrap_header">
      <!-- Menu -->
      <div class="wrap_menu">
        <nav class="menu">
          {% include 'main-menu' %}
        </nav>
      </div>
      <!-- Header Icon -->
      <div class="header-icons">
      </div>
    </div>
  </div>
  <!-- Header Mobile -->
  <div class="wrap_header_mobile">
    <!-- Logo moblie -->
    {% if section.settings.logo %}
    <a href="/" class="logo-mobile">
      <img src="{{ section.settings.logo | img_url: 'master' }}" alt="{{ section.settings.logo.alt | default: shop.name }}">
    </a>
    {% endif %}
    <!-- Button show menu -->
    <div class="btn-show-menu">
      <!-- Header Icon mobile -->
      <div class="header-icons-mobile">
        {% if shop.customer_accounts_enabled %}
            {% if customer %}
              <a href="/account" class="header-wrapicon1 dis-block">
                <img src="{{ 'icon-header-01.png' | asset_url }}" class="header-icon1" alt="ICON">
              </a>
            {% else %}
              <a href="/account/login" class="header-wrapicon1 dis-block">
                <img src="{{ 'icon-header-01.png' | asset_url }}" class="header-icon1" alt="ICON">
              </a>
            {% endif %}
        {% endif %}
        <span class="linedivide2"></span>
        {% include 'header-cart-mob' %}
      </div>
      <div class="btn-show-menu-mobile hamburger hamburger--squeeze">
        <span class="hamburger-box">
          <span class="hamburger-inner"></span>
        </span>
      </div>
    </div>
  </div>
  <!-- Menu Mobile -->
  <div class="wrap-side-menu" >
    <nav class="side-menu">
      <ul class="main-menu">
        <li class="item-topbar-mobile p-l-20 p-t-8 p-b-8">
          <span class="topbar-child1">
            {% if section.settings.use_top_bar %}
            {{ section.settings.top_bar_message }}
            {% endif %}
          </span>
        </li>
        <li class="item-topbar-mobile p-l-20 p-t-8 p-b-8">
          <div class="topbar-child2-mobile">
            {% if section.settings.show_email %}
            <span class="topbar-email">
              {{ section.settings.header_email_text }}
            </span>
            {% endif %}

            <div class="topbar-language rs1-select2">
              {% include 'header-currency' %}
            </div>
          </div>
        </li>
        <li class="item-topbar-mobile p-l-10">
          <div class="topbar-social-mobile">
            {% include 'social-button-header' %}
          </div>
        </li>
        {% include 'header-mob-nav' %}
      </ul>
    </nav>
  </div>
</header>
<!-- Slide1 -->
